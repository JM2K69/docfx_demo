{

  "build": {
    "sitemap": {
      "baseUrl": "",
      "priority": 0.1,
      "changefreq": "monthly"
    },
    "content": [
      {
        "files": [ "**/*.{md,yml}" ],
        "exclude": [ "_site/**", "obj/**" ]
      }
    ],
    "resource": [
      {
        "files": [ "**/images/**", "images/**", "**/ressources/**", "ressources/**" ]
      }
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "globalMetadata": {
      "_appTitle": "Demo docfx",
      "_enableSearch": "true",
      "_enableNewTabForExternalLink": "true",
      "_disableContribution": "true",
      "_appFooter":"docfx Demo Â© 2024",
      "_appLogoPath": "/images/logo.png",
      "_appFaviconPath": "../images/favicon.png",
      "_language": "fr",
      "pdf": true,
      "pdfTocPage": true
    },
    "markdownEngineProperties": {
      "markdigExtensions": [
        "Abbreviations",
        "Footnotes",
        "EmphasisExtras",
        "CustomContainers",
        "Footers",
        "TaskLists",
        "Bootstrap"
    ],
      "alerts": {
        "TODO": "alert alert-success"
      }
    },
    "dest": "_site",
    "template": [
      "default",
      "modern",
      "theme"
    ]
  }
}
